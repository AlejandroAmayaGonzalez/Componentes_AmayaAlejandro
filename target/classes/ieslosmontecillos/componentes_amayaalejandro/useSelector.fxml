<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root maxWidth="100" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1">
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="10.0">
            <Button fx:id="previousButton" mnemonicParsing="false" text="&lt;" />
            <Label fx:id="lb" text="Uno" contentDisplay="CENTER" />
            <Button fx:id="nextButton" mnemonicParsing="false" text="&gt;" />
      </HBox>
</fx:root>
